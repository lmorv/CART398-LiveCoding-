samples ({
  frogAmbient: 'https://cdn.freesound.org/previews/710/710245_10954437-lq.mp3',

  frosch: 'https://cdn.freesound.org/previews/275/275551_537412-lq.mp3',
  
  clicker: 'https://cdn.freesound.org/previews/203/203910_3564812-lq.mp3',

