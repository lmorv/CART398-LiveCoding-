  bitfrog: 'https://cdn.freesound.org/previews/8/8727_22332-lq.mp3'
  
})
stack(
  s("[<25 12 15 6>] [frogAmbient?0.6 3]?0.6 [clicker] <clicker frosch> frosch [lowcroack | bitfrog*2]").gain(.6),
  
  s("misc*6 bd, bd!6").bank("KorgMinipops"),
  
  note("[c d e . f g a]?0.7, [c d e . f g a]?0.5 ").s("gm_pad_poly").gain(.7),
  //note("[c d e . f g a]").s("gm_pad_poly").gain(.7),

  s("[- bd*2 - bd@3 <bd lt*3>], [- - bd*2 - bd@3 <bd lt*6>]").bank("SimmonsSDS5").gain(.4),
